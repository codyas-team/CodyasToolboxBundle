{% set pageTitle = pageTitle is defined ? pageTitle : ( 'Empty page' | trans) %}
{% set modules = codyas_tb_config('modules', true) %}
{% if modules == null %}
    {% set modules = codyas_mt2_modules() %}
{% endif %}
<!DOCTYPE html>
<html lang="en">
<!--begin::Head-->
<head>
    <base href="">
    <title>{{ pageTitle }} | {{ codyas_tb_config('app_name') }}</title>
    <meta name="description" content="{{ codyas_tb_config('app_description') }}">
    <meta name="keywords" content="{{ codyas_tb_config('app_keywords') }}">
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <meta charset="utf-8"/>
    <meta property="og:locale" content="en_US"/>
    <meta property="og:type" content="article"/>
    <meta property="og:title"
            content="{{ codyas_tb_config('app_name') }}"/>
    <meta property="og:url" content="{{ path(codyas_tb_config('home_path')) }}"/>
    <meta property="og:site_name" content="{{ codyas_tb_config('app_name') }}"/>
    <link rel="canonical" href="{{ path(codyas_tb_config('home_path')) }}"/>
    <link rel="shortcut icon" href="{{ asset(codyas_tb_config('logo')) }}"/>
    <!--begin::Fonts-->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700"/>
    <!--end::Fonts-->
    <!--begin::Page Vendor Stylesheets(used by this page)-->
    <!--end::Page Vendor Stylesheets-->
    <!--begin::Global Stylesheets Bundle(used by all pages)-->
    <link href="{{ asset('bundles/codyastoolbox/templates/mt2/plugins/global/plugins.dark.bundle.css') }}" rel="stylesheet" type="text/css"/>
    <link href="{{ asset('bundles/codyastoolbox/templates/mt2/css/style.dark.bundle.css') }}" rel="stylesheet" type="text/css"/>
    {% block stylesheets %}
        {{ encore_entry_link_tags('codyas_ep') }}
    {% endblock %}
    <!--end::Global Stylesheets Bundle-->
</head>
<!--end::Head-->
<!--begin::Body-->
<body id="kt_body" style="background-image: url({{ asset('bundles/codyastoolbox/templates/mt2/media/header-bg-dark.png') }})"
        data-scope="scope-{{ scope is defined ? scope : 'app' }}"
        class="dark header-fixed header-tablet-and-mobile-fixed toolbar-enabled">
<!--begin::Main-->
<!--begin::Root-->
<div class="d-flex flex-column flex-root">
    <!--begin::Page-->
    <div class="page d-flex flex-row flex-column-fluid {{ scope is defined ? scope : '' }}">
        <!--begin::Wrapper-->
        <div class="wrapper d-flex flex-column flex-row-fluid" id="kt_wrapper">
            {% block header %}
                {% include '@CodyasToolbox/templates/mt2/partials/_header.html.twig' with {'modules' : modules} %}
            {% endblock %}
            {% block toolbar %}
                <!--begin::Toolbar-->
                <div class="toolbar py-5 py-lg-15" id="kt_toolbar">
                    <!--begin::Container-->
                    <div id="kt_toolbar_container" class="container-xxl d-flex flex-stack flex-wrap">
                        <!--begin::Page title-->
                        <div class="page-title d-flex flex-column me-3">
                            <!--begin::Title-->
                            <h1 class="d-flex text-white fw-bolder my-1 fs-3">{{ pageTitle }}</h1>
                            <!--end::Title-->
{#                            <!--begin::Breadcrumb-->#}
{#                            <ul class="breadcrumb breadcrumb-separatorless fw-bold fs-7 my-1">#}
{#                                <!--begin::Item-->#}
{#                                <li class="breadcrumb-item text-white opacity-75">#}
{#                                    <a href="../../demo2/dist/index.html" class="text-white text-hover-primary">Home</a>#}
{#                                </li>#}
{#                                <!--end::Item-->#}
{#                                <!--begin::Item-->#}
{#                                <li class="breadcrumb-item">#}
{#                                    <span class="bullet bg-white opacity-75 w-5px h-2px"></span>#}
{#                                </li>#}
{#                                <!--end::Item-->#}
{#                                <!--begin::Item-->#}
{#                                <li class="breadcrumb-item text-white opacity-75">Dashboard</li>#}
{#                                <!--end::Item-->#}
{#                            </ul>#}
{#                            <!--end::Breadcrumb-->#}
                        </div>
                        <!--end::Page title-->
                        <!--begin::Actions-->
                        <div class="d-flex align-items-center py-3 py-md-1">
                            {% block pageActions %}{% endblock %}
                        </div>
                        <!--end::Actions-->
                    </div>
                    <!--end::Container-->
                </div>
                <!--end::Toolbar-->
            {% endblock %}

            {% block outterBody %}
                <div id="kt_content_container" class="d-flex flex-column-fluid align-items-start container-xxl ">
                    <!--begin::Post-->
                    <div class="content flex-row-fluid" id="kt_content">
                        <!--begin::Card-->
                        <div class="card">
                            <!--begin::Card header-->
                            <div class="card-header border-0 pt-6">
                                {% block cardHeader %}{% endblock %}
                            </div>
                            <!--end::Card header-->
                            <!--begin::Card body-->
                            <div class="card-body pt-0">
                                {% block content %}{% endblock %}
                            </div>
                            <!--end::Card body-->
                        </div>
                        <!--end::Card-->
                    </div>
                    <!--end::Post-->
                </div>
            {% endblock %}

            {% block footer %}
                {% include '@CodyasToolbox/templates/mt2/partials/_footer.html.twig' %}
            {% endblock %}

        </div>
        <!--end::Wrapper-->
    </div>
    <!--end::Page-->
</div>
<!--end::Root-->
<!--begin::Drawers-->
{% block drawers %}
    {% if modules.activities.enabled and modules.activities.drawer_template %}
        {% include modules.activities.drawer_template %}
    {% endif %}
    {% if modules.chat.enabled and modules.chat.drawer_template %}
        {% include modules.chat.drawer_template %}
    {% endif %}
{% endblock %}
{% block modals %}{% endblock %}

<!--end::Drawers-->
<!--begin::Scrolltop-->
<div id="kt_scrolltop" class="scrolltop" data-kt-scrolltop="true">
    <!--begin::Svg Icon | path: icons/duotune/arrows/arr066.svg-->
    <span class="svg-icon">
				<svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
					<rect opacity="0.5" x="13" y="6" width="13" height="2" rx="1" transform="rotate(90 13 6)"
                            fill="black"/>
					<path d="M12.5657 8.56569L16.75 12.75C17.1642 13.1642 17.8358 13.1642 18.25 12.75C18.6642 12.3358 18.6642 11.6642 18.25 11.25L12.7071 5.70711C12.3166 5.31658 11.6834 5.31658 11.2929 5.70711L5.75 11.25C5.33579 11.6642 5.33579 12.3358 5.75 12.75C6.16421 13.1642 6.83579 13.1642 7.25 12.75L11.4343 8.56569C11.7467 8.25327 12.2533 8.25327 12.5657 8.56569Z"
                            fill="black"/>
				</svg>
			</span>
    <!--end::Svg Icon-->
</div>
<!--end::Scrolltop-->
<!--end::Main-->
<script>var hostUrl = "assets/";</script>
<!--begin::Javascript-->
<!--begin::Global Javascript Bundle(used by all pages)-->
<script src="{{ asset('bundles/codyastoolbox/templates/mt2/plugins/global/plugins.dark.bundle.js') }}"></script>
<script src="{{ asset('bundles/codyastoolbox/templates/mt2/js/scripts.bundle.js') }}"></script>
<!--end::Global Javascript Bundle-->
<!--begin::Page Vendors Javascript(used by this page)-->
<!--end::Page Vendors Javascript-->
<!--begin::Page Custom Javascript(used by this page)-->
<script src="{{ asset('bundles/codyastoolbox/templates/mt2/js/custom/widgets.js') }}"></script>
{% block javascripts %}
    {{ encore_entry_script_tags('codyas_ep') }}
{% endblock %}
<!--end::Page Custom Javascript-->
<!--end::Javascript-->
</body>
<!--end::Body-->
</html>